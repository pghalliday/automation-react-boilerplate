{
  "rules": {
    ".read": false,
    ".write": false,
    "sessions": {
      ".read": false,
      ".write": false
    },
    "users": {
      "$user_id": {
        ".read": "$user_id === auth.uid",
        ".write": "$user_id === auth.uid",
        ".validate": "newData.hasChildren(['approved'])",
        "approved": {
          ".validate": "newData.val() === false"
        },
        "$other": {
          ".validate": false
        }
      }
    }
  }
}
